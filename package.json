  "name": "caramella-corner",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "build": "vite build && mkdir -p .output && cp instrument.server.mjs .output/server",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "db:generate": "drizzle-kit generate",
